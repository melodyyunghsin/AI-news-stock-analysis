{
  "manifest_version": 3,
  "name": "Stock Predictor with News Analysis",
  "version": "1.0",
  "description": "Analyzes news articles and predicts stock impacts.",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [
  {
    "resources": ["extension_data/ticker_reliability.json"],
    "matches": ["<all_urls>"]
  }
]
}